{
  "manifest_version": 3,
  "name": "Google Account Switcher",
  "version": "1.0",
  "description": "Switch between Google user accounts.",
  "permissions": ["tabs", "<all_urls>"],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Switch Google Account"
  },
  "background": {
    "service_worker": "user-switch.js"
  },
  "icons": {
    "48": "icon.png"
  },
  "host_permissions": [
    "*://*.google.com/*"
  ]
}
